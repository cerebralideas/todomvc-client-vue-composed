    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.1",
    "husky": "^3.1.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "miragejs": "^0.1.33",
    "playwright": "^0.9.24",
    "prettier": "^1.19.1",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.12"
  }
